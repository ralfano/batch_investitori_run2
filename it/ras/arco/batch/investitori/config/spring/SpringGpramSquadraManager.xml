    <bean id="GetGpramSquadra"
    	class="it.ras.arco.batch.service.GpramSquadraManagerSRV" singleton="false">
    	<property name="moduleManager">
    		<ref bean="RasModuleManager" />
    	</property>
    	<property name="action">
    		<value>GET</value>
    	</property>
    </bean>
   
    <bean id="InsGpramSquadra"
    	class="it.ras.arco.batch.service.GpramSquadraManagerSRV" singleton="false">
