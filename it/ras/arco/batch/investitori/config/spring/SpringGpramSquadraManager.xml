    	</property>
    </bean>
    

	<bean id="CheckGetSquadraGpramMD"    
    class="it.ras.flag.module.DefaultDbModule" >
    <property name="dao">
    		<ref bean="RasBatchDao" />
    	</property>
    	<property name="query">
    		<value>0</value>
    	</property>
    	<property name="queryName">
    		<value>GetSquadra</value>
    	</property>
    	<property name="parameters">
