<property name="denominazione"            type="string"  column="DENOMINAZIONE"                length="50"/>  
<property name="codFisc"            type="string"  column="CODICE_FISCALE"                length="16"/>  
<property name="Piva"            type="string"  column="PARTITA_IVA"                length="11"/> 
<property name="locNasc"            type="string"  column="LOCALITA_NASCITA"                length="30"/> 
<property name="provNasc"            type="string"  column="PROV_NASCITA"                length="2"/> 
<property name="statoNasc"            type="string"  column="STATO_NASCITA"                length="3"/> 
<property name="sesso"            type="string"  column="SESSO"                length="1"/> 
