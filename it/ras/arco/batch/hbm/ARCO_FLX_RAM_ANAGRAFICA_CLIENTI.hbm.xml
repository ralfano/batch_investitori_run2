<property name="capRes"            type="string"  column="CAP_RES "                length="5"/>  
<property name="statoRes"            type="string"  column="STATO_RES"                length="3"/>  
<property name="tipoDoc"            type="string"  column="TIPO_DOCUMENTO"                length="1"/>  
<property name="numDoc"            type="string"  column="NUM_DOCUMENTO"                length="15"/>
 <property name="enteRilasc"            type="string"  column="ENTE_RILASCIO"                length="25"/>  
<property name="codSgr"            type="string"  column="COD_SGR"                    length="1"/> 
<property name="compDocRisc"            type="string"  column="COMP_DOC_RISC"              length="1"/>
<property name="concAut"            type="string"  column="CONC_AUT"                   length="1"/>
<property name="regisConflAge"            type="string"  column="REGIS_CONFL_INTERES"        length="1"/>
<property name="flgSpedAge"            type="string"  column="FLG_SPED_AGE"                length="1"/>
