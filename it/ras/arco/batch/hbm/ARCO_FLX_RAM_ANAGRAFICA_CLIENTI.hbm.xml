<property name="settore"              type="integer" column="SETTORE"/>              
<property name="attEconPrisma"              type="integer" column="ATT_ECON_PRISMA"/>            
<property name="untUltManut"            type="string"  column="UTN_ULT_MANUT"                length="4"/>  
