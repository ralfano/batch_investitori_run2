<property name="Nazionalita"            type="string"  column="NAZIONALITA"                length="15"/>
<property name="cabRes"            type="string"  column="CAB_RES"                length="6"/>
<property name="comRes"            type="string"  column="COMUNE_RES"                length="25"/>
<property name="flagForzCf"            type="string"  column="FLAG_FORZ_CF"                length="1"/>
<property name="indirizzoRes"            type="string"  column="INDIRIZZO_RES"                length="40"/>
<property name="dataAutMarket"        type="date"    column="DT_AUT_DAT_MARKET"/>
<property name="dataUltManut"        type="date"    column="DT_ULT_MANUT"/>     
<property name="dataConflInt"        type="date"    column="DT_CONFL_INTERES"/> 
<property name="dataComplDocRisc"        type="date"    column="DT_COMPL_DOC_RISC"/>
<property name="dataAutPriv"        type="date"    column="DT_AUT_PRIVACY"/>   
<property name="dataIns"        type="date"    column="DT_INS"/>           
<property name="dataNasc"        type="date"    column="DT_NASCITA"/>       
<property name="dataRil"        type="date"    column="DATA_RILASCIO"/>    
<property name="statoElab"              type="string"  column="STATO_ELAB_B"               length="2"/>           
<property name="descrizioneErrore"      type="string"  column="DESCRIZIONE_ERRORE_S"       length="130"/> 
<property name="dataElab"               type="timestamp"    column="DATA_ELAB_D"/>                        
  
 </class>
 <query name="ANAGRAFICA_RAM_CURS">from it.ras.arco.batch.bean.FlxRamAnagraficaClienti pc where pc.statoElab='D' </query>
</hibernate-mapping>
