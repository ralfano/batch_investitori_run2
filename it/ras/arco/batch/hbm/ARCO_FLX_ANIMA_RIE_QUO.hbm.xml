<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="it.ras.arco.batch.bean.FlxAnimaRieQuo" rowid="rowid" table="FLX_ANIMA_RIE_QUO">
  <id name="flxId" column="FLX_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FLX_ANIMA_RIE_QUO_SEQ</param>
   </generator>
  </id>
  <property name="valoreFisso1" type="string" column="VALORE_FISSO1" length="5"/>
  <property name="filler" type="string" column="FILLER_1" length="9"/>
  <property name="codiceFondo" type="string" column="CODICE_FONDO" length="6"/>
  <property name="valoreQuota" type="double" column="VALORE_QUOTA"/>
  <property name="valoreFisso2" type="string" column="VALORE_FISSO2" length="8"/>
  <property name="valoreFisso3" type="string" column="VALORE_FISSO3" length="3"/>
  <property name="dataValoreQuota" type="date" column="DATA_VALORE_QUOTA"/>
  <property name="valoreFisso4" type="string" column="VALORE_FISSO4" length="3"/>
  <property name="codSocCollocatrice" type="string"
   column="COD_SOC_COLLOCATRICE" length="5"/>
  <property name="abiSocCollocatrice" type="string"
   column="ABI_SOC_COLLOCATRICE" length="5"/>
  <property name="statoElab" type="string" column="STATO_ELAB_B" length="2"/>
  <property name="istanza" type="string" column="ISTANZA_S" length="1"/>
  <property name="dataCaricamento" type="date" column="DATA_CARICAMENTO_S"/>
  <property name="descrizioneErrore"              column="DESCRIZIONE_ERRORE_S"           type="string" length="100"  />
  <property name="dataErrore"                     column="DATA_ERRORE"                    type="date" />
 </class>
 <query name="ANIMA_RIE_QUO">from
it.ras.arco.batch.bean.FlxAnimaRieQuo pc where pc.statoElab='D' order by pc.flxId</query>
</hibernate-mapping>
