<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="it.ras.arco.batch.bean.FlxAntCrm1" rowid="rowid" table="FLX_ANT_CRM1">
  <id name="flxId" column="FLX_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FLX_ANT_CRM1_SEQ</param>
   </generator>
  </id>
  <property name="codiceAgente" type="string" column="COD_AGENTE_S" length="7"/>
  <property name="codiceProdottoEsterno" type="string" column="COD_PRODOTTO_EXT_S" length="20"/>
  <property name="codiceLinea" type="string" column="COD_LINEA_S" length="10"/>
  <property name="codiceMandato" type="string" column="COD_MANDATO_S" length="15"/>
  <property name="numeroProtocollo" type="string" column="NUM_PROTOCOLLO_S" length="15"/>
  <property name="modalitaSottoscrizione" type="string" column="MOD_SOTTOSCRIZIONE_B" length="1"/>
  <property name="causaleConferimentoSmobilizzo" type="string" column="CAUSALE_CONF_SMOB_T" length="3"/>
  <property name="causaleProvvigionale" type="string" column="CAUSALE_PROVV_T" length="3"/>
  <property name="dataCompetenza" type="date" column="DATA_COMPETENZA_D"/>
  <property name="dataRegolamento" type="date" column="DATA_REGOLAMENTO_D"/>
  <property name="importoBaseSpese" type="double" column="IMP_BS_C"/>
  <property name="importoPayIn" type="double" column="IMP_PAY_IN_C"/>
  <property name="importoProvvigioniDiretteFront" type="double" column="IMP_DIR_FRONT_C"/>
  <property name="importoProvvigioniOverFront" type="double" column="IMP_OVER_FRONT_C"/>
  <property name="flagDiretteOver" type="string" column="FLG_DIR_OVER_B" length="1"/>
  <property name="societaProdotto" type="string" column="SOCIETA_PRODOTTO_S" length="50"/>
  <property name="tipoProdotto" type="string" column="TIPO_PRODOTTO_T" length="3"/>
  <property name="codiceRete" type="string" column="COD_RETE_T" length="3"/>
  <property name="codiceCompagnia" type="string" column="COD_COMP_T" length="3"/>
  <property name="gradoGerarchico" type="string" column="GRADO_S" length="3"/>
  <property name="descrizioneErrore" type="string" column="DESCRIZIONE_ERRORE_S" length="100" />
  <property name="dataErrore" type="date" column="DATA_ELAB_D" />
  <property name="statoElaborazione" type="string" column="STATO_ELAB_B" length="2"/>
  <property name="istanza" type="string" column="ISTANZA_S" length="1"/>
  <property name="dataCaricamento" type="date" column="DATA_CARICAMENTO_S"/>
  </class>
  	
 <query name="CRM1_ANT">from it.ras.arco.batch.bean.FlxAntCrm1 c where c.statoElaborazione='D'</query>
</hibernate-mapping>
