<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA ATT_FIN_PROD -->
 <class name="it.ras.arco.batch.bean.FlxIndcoicr" table="FLX_INDCOICR" where="STATO_ELAB_B='D'">
  <id name="attivita_ProdID" column="FLX_ID" type="java.lang.Integer">
   <generator class="sequence">
    <param name="sequence">FLX_INDCOICR_SEQ</param>
   </generator>
  </id>
  <property name="dtulm" type="date" column="DTULM"/>
  <property name="utent" type="string" column="UTENT" length="4"/>
  <property name="fondo" type="integer" column="FONDO"/>
  <property name="manda" type="string" column="MANDA" length="11"/>
  <property name="clcli" type="string" column="CLCLI"/>
  <property name="indco" type="string" column="INDCO" length="40"/>
  <property name="locco" type="string" column="LOCCO" length="40"/>
  <property name="proco" type="string" column="PROCO" length="2"/>
  <property name="capco" type="string" column="CAPCO" length="5"/>
  <property name="staco" type="string" column="STACO" length="3"/>
  <property name="tprec" type="integer" column="TPREC"/>
  <property name="tpcli" type="integer" column="TPCLI"/>
  <property name="codsq" type="string" column="CODSQ"/>
  <property name="gstdStatoElab" type="string" column="STATO_ELAB_B" length="2"/>
  <property name="istanza" type="string" column="ISTANZA_S" length="2"/>
  
  <property name="descrizioneErrore" type="string" column="DESCRIZIONE_ERRORE_S" length="100"/>
  <property name="dataElaborazione" type="timestamp" column="DATA_ELAB_D"/>  
 </class>
 <query name="QUERY_INDCOICR_SQL">from it.ras.arco.batch.bean.FlxIndcoicr order by dtulm, manda</query>
</hibernate-mapping>
