<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA INDIRIZZI SY -->
 <class name="it.ras.arco.batch.bean.FlxQuadraturaSyMig" rowid="rowid"
  table="FLX_QUADRATURA_SY_MIG"
	dynamic-update="true" dynamic-insert="true" batch-size="100" lazy="true" >
  <id name="flxId" column="FLX_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FLX_QUADRATURA_SY_MIG_SEQ</param>
   </generator>
  </id>
  <property name="codFabbrica" type="string" column="COD_FABBRICA" length="5"/>
  <property name="tipoProdotto" type="integer" column="TIPO_PRODOTTO"/>
  <property name="numSottoscrizione" type="integer" column="NUM_SOTTOSCRIZIONE"/>
  <property name="numContrattoSec" type="string" column="NUM_CONTRATTO_SEC" length="25"/>
  <property name="numDossierTecnicoFabbrica" type="string" column="NUM_DOSSIER_TECNICO_FABBRICA" length="25"/>
  <property name="codCompartoSec" type="integer" column="COD_COMPARTO_SEC"/>
  <property name="codCompartoFabbrica" type="string" column="COD_COMPARTO_FABBRICA" length="5"/>
  <property name="numQuote" type="integer" column="NUM_QUOTE" />
  <property name="isin" type="string" column="ISIN" length="12"/>
  <property name="rapporto" type="integer" column="RAPPORTO" />
  <property name="statoElabB" type="string" column="STATO_ELAB_B" length="2"/>
  <property name="istanzaS" type="string" column="ISTANZA_S" length="1"/>
  <property name="descrizioneErrore"              column="DESCRIZIONE_ERRORE_S"           type="string" length="100"  />
  <property name="dataErrore"                     column="DATA_ERRORE"                    type="date" />
 </class>
 <query name="CURSOR_QUADRATURA">from it.ras.arco.batch.bean.FlxQuadraturaSyMig a where a.statoElabB='D' </query>
</hibernate-mapping>
