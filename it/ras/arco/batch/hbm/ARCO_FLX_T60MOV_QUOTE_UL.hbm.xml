<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

 <class name="it.ras.arco.batch.bean.FlxT60MovQuoteUl" rowid="rowid"
  table="FLX_T60MOV_QUOTE_UL" where="STATO_ELAB_B='D'">
  <id name="flxId" column="FLX_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FLX_GENERIC_SEQ</param>
   </generator>
  </id>

<property name="compagnia" type="integer" column="COMPAGNIA"/>
<property name="ramo" type="integer" column="RAMO"/>
<property name="tipo" type="integer" column="TIPO"/>
<property name="numPolizza" type="string" column="NUM_POLIZZA" length="11"/>
<property name="progOperaz" type="integer" column="PROG_OPERAZ"/>
<property name="tipoOperaz" type="integer" column="TIPO_OPERAZ"/>
<property name="codMotOperaz" type="integer" column="COD_MOT_OPERAZ"/>
<property name="progTabella" type="integer" column="PROG_TABELLA"/>
<property name="progGaranziaUnit" type="integer" column="PROG_GARANZIA_UNIT"/>
<property name="codiceTitolo" type="integer" column="CODICE_TITOLO"/>
<property name="progDelTitolo" type="integer" column="PROG_DEL_TITOLO"/>
<property name="progDellaComunic" type="integer" column="PROG_DELLA_COMUNIC"/>
<property name="progDellaComunic1" type="integer" column="PROG_DELLA_COMUNIC1"/>
<property name="progFondo" type="integer" column="PROG_FONDO"/>
<property name="codFondo" type="string" column="COD_FONDO" length="10"/>
<property name="codLinea" type="string" column="COD_LINEA" length="8"/>
<property name="dataInizioEffetto" type="string" column="DATA_INIZIO_EFFETTO" length="10"/>
<property name="acquistoVendita" type="string" column="ACQUISTO_VENDITA" length="1"/>
<property name="stato" type="integer" column="STATO"/>
<property name="tipoInvestim" type="string" column="TIPO_INVESTIM" length="1"/>
<property name="codValuta" type="string" column="COD_VALUTA" length="3"/>
<property name="premioInvestire" type="double" column="PREMIO_INVESTIRE" length="15"/>
<property name="impLordoTitolo" type="double" column="IMP_LORDO_TITOLO" length="15"/>
<property name="impPremioPuro" type="double" column="IMP_PREMIO_PURO" length="15"/>
<property name="commissioni" type="double" column="COMMISSIONI" length="15"/>
<property name="numQuoteFondo" type="double" column="NUM_QUOTE_FONDO" length="15"/>
<property name="numQuoteInvest" type="double" column="NUM_QUOTE_INVEST" length="15"/>
<property name="numQuoteRiservate" type="double" column="NUM_QUOTE_RISERVATE"/>
<property name="dataValCambio" type="string" column="DATA_VAL_CAMBIO" length="10"/>
<property name="dataComunica1" type="string" column="DATA_COMUNICA1" length="10"/>
<property name="dataComunica2" type="string" column="DATA_COMUNICA2" length="10"/>
<property name="dataRegisOperaz" type="string" column="DATA_REGIS_OPERAZ" length="10"/>
<property name="statoElabB" type="string" column="STATO_ELAB_B" length="2"/>
<property name="istanzaS" type="string" column="ISTANZA_S" length="1"/>
<property name="dataCaricamentoS" type="date" column="DATA_CARICAMENTO_S"/>
  </class>
<query name="GetFlxT60MovQuotrUlByPolizza">select s from it.ras.arco.batch.bean.FlxT60MovQuoteUl s where s.numPolizza = ? </query>

</hibernate-mapping>
