<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA ANAGRAFICA CC -->
 <class name="it.ras.arco.batch.bean.FlxBpuMo1" rowid="rowid" table="FLX_BPU_MO1">
  <id name="flxId" column="FLX_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FLX_BPU_MO1_SEQ</param>
   </generator>
  </id>
<property name="nprgflu"                   type="integer"                  column="NPRGFLU"    />             	
<property name="nprgrec"                   type="integer"                  column="NPRGREC"     />            	
<property name="cscv"                      type="integer"                  column="CSCV"       />             	
<property name="cclc"                      type="integer"                  column="CCLC"       />             	
<property name="csot"                      type="integer"                  column="CSOT"       />             	
<property name="csotest"                   type="string"                   column="CSOTEST"        length="25"  /> 
<property name="trec"                      type="string"                   column="TREC"  length="3"  />         	
<property name="cmpa"                      type="string"                   column="CMPA"           length="3"  />  
<property name="imov"                      type="double"                   column="IMOV"     />                	
<property name="cabi"                      type="integer"                  column="CABI"      />               	
<property name="ccab"                      type="integer"                  column="CCAB"      />               	
<property name="ccco"                      type="string"                   column="CCCO"         length="46"  />        
<property name="ccro"                      type="integer"                  column="CCRO"    />                	
<property name="dval"                      type="date"                     column="DVAL"  />                  	
<property name="cdiv"                      type="string"                   column="CDIV"                   length="3" 	/>	
<property name="idTemporaneoMovimento"     type="integer"                  column="ID_TEMPORANEO_MOVIMENTO"	            />  
<property name="statoElab"                 type="string"                   column="STATO_ELAB_B"           length="2" 	/>	
<property name="istanza"                   type="string"                   column="ISTANZA_S"              length="1" 	/>	
                                                                                					

 </class>
 <sql-query name="GetBpuMo1">
<return alias="bpuMo1" class="it.ras.arco.batch.bean.FlxBpuMo1"/>
select {bpuMo1.*} from FLX_BPU_MO1 bpuMo1    
 where
 bpuMo1.ID_TEMPORANEO_MOVIMENTO = :IdTempMov
 and bpuMo1.NPRGFLU = :NprgFlu  
 
</sql-query>
</hibernate-mapping>
