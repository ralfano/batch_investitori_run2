<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA DEL SALDO PERIODICO -->
 <class select-before-update="false" dynamic-update="true" rowid="rowid" name="it.ras.arco.bean.FinSaldoPeriodico" table="FIN_SALDO_PERIODICO" where="GSTD_ESIST_B='S'">
  <id name="saldoPeriodicoId" column="SALDO_PERIODICO_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_SALDO_PERIODICO_SEQ</param>
   </generator>
  </id>
<property name="contrattoId"                    column="CONTRATTO_ID"                   type="integer"  />
<property name="periodo"                        column="PERIODO"                        type="integer"  />
<property name="tipoContratto"                  column="TIPO_CONTRATTO_S"               type="string" length="2"  />
<property name="giacenza"                       column="GIACENZA"                       type="double"  />
<property name="dataInizio"                     column="DATA_INIZIO_D"                  type="date"  />
<property name="dataFine"                       column="DATA_FINE_D"                    type="date"  />
<property name="annoRif"                        column="ANNO_RIF"                       type="integer"  />
<property name="meseRif"                        column="MESE_RIF"                       type="integer"  />
<property name="giornoRif"                      column="GIORNO_RIF"                     type="integer"  />
<property name="flagSubagente"                  column="FLAG_SUBAGENTE"                 type="string" length="1" />
<property name="flagSinergia"                   column="FLAG_SINERGIA"                  type="string" length="1"/>
<property name="delta"                          column="DELTA_N"                        type="double"  />
<property name="flagEstinzione"                 column="FLAG_ESTINZIONE"                type="string" length="1"  />
<property name="commissioni"                    column="COMMISSIONI"                    type="double"  />
<property name="spese"                          column="SPESE"                          type="double"  />
<property name="datval"                         column="DATVAL"                         type="date"  />
<property name="proswi"                         column="PROSWI"                         type="string" length="2" />
<property name="pernav"                         column="PERNAV"                         type="double"  />
<property name="fido"                           column="FIDO"                           type="double"  />
<property name="importo"                        column="IMPORTO"                        type="double"  />
<property name="userUltimaModifica"             column="GSTD_ULT_USER_S"                type="string" length="40"  />
<property name="proceduraUltimaModifica"        column="GSTD_ULT_PROC_S"                type="string" length="40"  />
<property name="dataUltimaModifica"             column="GSTD_ULT_MOD_D"                 type="date"  />
<property name="dataInserimento"                column="GSTD_INSE_D"                    type="date"  />
<property name="flagEsistenza"                  column="GSTD_ESIST_B"                   type="string" length="1"  />
<property name="utenteInserimento"              column="GSTD_USER_INS_S"                type="string" length="40"  />
<property name="tipoUltimaModifica"             column="GSTD_TIPO_MOD_B"                type="string" length="1"  />

<property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
<property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
<property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
<property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
</class>
</hibernate-mapping>
