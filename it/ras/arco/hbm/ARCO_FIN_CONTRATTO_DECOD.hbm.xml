<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA DEI CONTRATTI -->
 <class name="it.ras.arco.bean.FinContrattoDecod" table="FIN_CONTRATTO_DECOD" lazy="true" where="GSTD_ESIST_B='S'">
  <id name="contattoDecodId" column="CONTRATTO_DECOD_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_CONTRATTO_DECOD_SEQ</param>
   </generator>
  </id>
 
  <property name="userUltimaModifica" type="string" column="GSTD_ULT_USER_S" length="20"/>
  <property name="proceduraUltimaModifica" type="string" column="GSTD_ULT_PROC_S" length="20"/>
  <property name="dataUltimaModifica" type="timestamp" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimento" type="timestamp" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B" length="1"/>
  <property name="utenteInserimento" type="string" column="GSTD_USER_INS_S" length="20"/>
  <property name="tipoUltimaModifica" type="string"   column="GSTD_TIPO_MOD_B" length="1"/>
	
  <property name="contrattoId" type="string" column="CONTRATTO_ID" length="30"/>
  <property name="decodNdgFabbrNewId" type="string" column="DECOD_NDG_FABBR_NEW_ID" length="30"/>
  <property name="decodNdgFabbrOldId" type="string"   column="DECOD_NDG_FABBR_OLD_ID" length="30"/>
		
 <!-- 
  <many-to-one name="contratto" class="it.ras.arco.bean.FinContratto" column="CONTRATTO_ID" not-found="ignore" lazy="true" cascade="lock" />
  <many-to-one name="decodNdgFabbrNew" class="it.ras.arco.bean.FinDecodNdgFabbr" column="DECOD_NDG_FABBR_NEW_ID" lazy="true" not-found="ignore" cascade="lock" />
  <many-to-one name="decodNdgFabbrOld" class="it.ras.arco.bean.FinDecodNdgFabbr" column="DECOD_NDG_FABBR_OLD_ID"  lazy="true" not-found="ignore" cascade="lock" />
 -->

  
</class>
