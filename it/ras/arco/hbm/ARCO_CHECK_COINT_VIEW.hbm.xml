<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- VISTA DELLE COINTESTAZIONI-->
 <class name="it.ras.arco.bean.CheckCointView"
  table="CHECK_COINT_VIEW" where= "GSTD_ESIST_B=&apos;S&apos;">
  <id name="viewId" column="VIEW_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">VIEW_SEQ</param>
   </generator>
  </id>
  <property name="soggCoin" type="integer" column="SOGG_COIN_N"/>
  <property name="cointSign" type="string" column="COINT_SIGN" length="200"/>
  <property name="userUltimaModifica" type="string" column="GSTD_ULT_USER_S"/>
  <property name="proceduraUltimaModifica" type="string" column="GSTD_ULT_PROC_S"/>
  <property name="dataUltimaModifica" type="timestamp" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimento" type="timestamp" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B"/>
  <property name="utenteInserimento" type="string" column="GSTD_USER_INS_S"/>
  <property name="tipoUltimaModifica" type="string" column="GSTD_TIPO_MOD_B"/>
 
   <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
 </class>
 <!--
 <query name="Check_Cointestazione">from it.ras.arco.bean.CheckCointView c  where c.cointSign = ?</query>
 -->
</hibernate-mapping>

