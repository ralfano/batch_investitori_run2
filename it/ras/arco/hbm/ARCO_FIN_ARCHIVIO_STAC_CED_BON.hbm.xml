<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 
 <class name="it.ras.arco.bean.FinArchivioStacCedBon"
  table="FIN_ARCHIVIO_STAC_CED_BON" where="GSTD_ESIST_B='S'">
  <id name="archivioStacCedBonId" column="ARCHIVIO_STAC_CED_BON_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_ARCHIVIO_STAC_CED_BON_SEQ</param>
   </generator>
  </id>
  
  

<property name="numeroMandato"                  column="NUMERO_MANDATO"                 type="string" length="11"  />
<property name="codiceAbi"                      column="CODICE_ABI"                     type="string" length="5"  />
<property name="codiceCab"                      column="CODICE_CAB"                     type="string" length="5"  />
<property name="cinIban"                        column="CIN_IBAN"                       type="string" length="1"  />
<property name="checkDigitIban"                 column="CHECK_DIGIT_IBAN"               type="string" length="2"  />
<property name="codicePaeseIban"                column="CODICE_PAESE_IBAN"              type="string" length="2"  />
<property name="numeroContoCorrente"            column="NUMERO_CONTO_CORRENTE"          type="string" length="15"  />
<property name="codiceIntestatarioBonifico"     column="CODICE_INTESTATARIO_BONIFICO"   type="string" length="7"  />
<property name="codiceSocieta"                  column="CODICE_SOCIETA"                 type="string" length="3"  />
<property name="percentualeEnte"                column="PERCENTUALE_ENTE"               type="double"  />
<property name="importoEnte"                    column="IMPORTO_ENTE"                   type="double"  />
<property name="durata"                         column="DURATA"                         type="string" length="3"  />
<property name="dataInizioErogazione"           column="DATA_INIZIO_EROGAZIONE"         type="date"  />
<property name="fondoEtico"                     column="FONDO_ETICO"                    type="string" length="1"  />
<property name="flagGestioneEccedenza"          column="FLAG_GESTIONE_ECCEDENZA"        type="string" length="1"  />
<property name="contrattoId"                    column="CONTRATTO_ID"                   type="integer"  />
<property name="userUltimaModifica"             column="GSTD_ULT_USER_S"                type="string" length="40"  />
<property name="proceduraUltimaModifica"        column="GSTD_ULT_PROC_S"                type="string" length="40"  />
<property name="dataUltimaModifica"             column="GSTD_ULT_MOD_D"                 type="date"  />
<property name="dataInserimento"                column="GSTD_INSE_D"                    type="date"  />
<property name="flagEsistenza"                  column="GSTD_ESIST_B"                   type="string" length="1"  />
<property name="utenteInserimento"              column="GSTD_USER_INS_S"                type="string" length="40"  />
<property name="tipoUltimaModifica"             column="GSTD_TIPO_MOD_B"                type="string" length="1"  />
 <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
 </class>

<query name="GetArchivioByContrattoId">from it.ras.arco.bean.FinArchivioStacCedBon s where s.contrattoId=?</query>
</hibernate-mapping>

