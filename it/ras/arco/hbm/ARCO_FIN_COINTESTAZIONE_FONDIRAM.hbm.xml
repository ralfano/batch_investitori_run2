<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA COINTESTAZIONI ANAGRAFICHE -->
 <class name="it.ras.arco.bean.FinCointestazione"
  table="FIN_COINTESTAZIONE" where="GSTD_ESIST_B=&apos;S&apos;">
  <id name="cointestazioneId" column="COINTESTAZIONE_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_COINTESTAZIONE_SEQ</param>
   </generator>
  </id>
  <!-- <property name="soggCoin" type="integer" column="SOGG_COIN_N"/> -->
  <!-- <property name="soggettoId" type="integer" column="SOGGETTO_ID"/> -->
  <property name="ordine" type="integer" column="ORDINE_N"/>
  <property name="tipo" type="string" column="TIPO_T" length="3"/>
  <property name="stato" type="string" column="STATO_T" length="3"/>
  <!--  <property name="intestazione" type="string" column="INTESTAZIONE_S" length="100"/>
 -->
  <property name="dataInizio" type="timestamp" column="DATA_INIZIO_D"/>
  <property name="dataFine" type="timestamp" column="DATA_FINE_D"/>
  <property name="userUltimaModifica" type="string"
   column="GSTD_ULT_USER_S" length="20"/>
  <property name="proceduraUltimaModifica" type="string"
   column="GSTD_ULT_PROC_S" length="20"/>
  <property name="dataUltimaModifica" type="timestamp" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimento" type="timestamp" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B" length="1"/>
  <property name="utenteInserimento" type="string"
   column="GSTD_USER_INS_S" length="20"/>
  <property name="tipoUltimaModifica" type="string"
   column="GSTD_TIPO_MOD_B" length="1"/>
    <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
   <!-- RELAZIONE CON FIN_SOGGETTO-->
  <many-to-one name="soggetto"
   class="it.ras.arco.bean.FinSoggetto"  column="SOGGETTO_ID" not-null="false" lazy="false" not-found="ignore" />
   <!-- RELAZIONE CON FIN_SOGGETTO -->
  <many-to-one name="soggettoCoin"
   class="it.ras.arco.bean.FinSoggetto"  column="SOGG_COIN_N"  not-null="false" lazy="false" not-found="ignore" />
  
 </class>
  <query name="GetCointestazione">from it.ras.arco.bean.FinCointestazione c where c.soggettoCoin=?</query> 
 
</hibernate-mapping>
