<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA LEGAME SOGGETTO-INDIRIZZO  -->

 <class name="it.ras.arco.bean.FinSoggettoIndir" table="FIN_SOGGETTO_INDIR" where="GSTD_ESIST_B='S'"
 		batch-size="100"
 		>
 
  <id name="soggettoIndirId" column="SOGGETTO_INDIR_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_SOGGETTO_INDIR_SEQ</param>
   </generator>
  </id>

  <property name="tipoIndirizzo" type="string" column="TIPO_INDIRIZZO_T" length="3"/>
  <property name="statoVariaz" type="string" column="STATO_VARIAZ_T" length="3"/>
  <property name="flgDaCanc" type="string" column="FLG_DA_CANC_B" length="1"/>
  <property name="presNote" type="string" column="PRES_NOTE_B" length="1"/>
  <property name="dataInizio" type="timestamp" column="DATA_INIZIO_D"/>
  <property name="dataFine" type="timestamp" column="DATA_FINE_D"/>
  <property name="dominato" type="string" column="DOMINATO_B" length="1"/>
  <property name="datiSupl" type="string" column="DATI_SUPL_S" length="10"/>
  <property name="userUltimaModifica" type="string"
   column="GSTD_ULT_USER_S" length="20"/>
  <property name="proceduraUltimaModifica" type="string"
   column="GSTD_ULT_PROC_S" length="20"/>
  <property name="dataUltimaModifica" type="timestamp" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimento" type="timestamp" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B" length="1"/>
  <property name="utenteInserimento" type="string"
   column="GSTD_USER_INS_S" length="20"/>
  <property name="tipoUltimaModifica" type="string"
   column="GSTD_TIPO_MOD_B" length="1"/>
     <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
 
 
   <many-to-one name="soggetto"
    	class="it.ras.arco.bean.FinSoggetto" column="SOGGETTO_ID" cascade="save-update,lock"  
    	fetch="select" not-found="ignore" 
    	/>
    	
   <many-to-one name="indirizzo"
    	class="it.ras.arco.bean.FinIndirizzo" column="INDIRIZZO_ID" 
    	cascade="save-update,lock"  not-found="ignore" 
    	fetch="select"
    	/>

 
 
  
 </class>
 
 <query name="GetIndirizzoBySoggettoId">select sin from it.ras.arco.bean.FinSoggettoIndir sin  
  join sin.soggetto s 
  	where s.soggettoId = ?
	and sin.tipoIndirizzo = ?
	and sin.dataFine > ? </query>
	
</hibernate-mapping>

