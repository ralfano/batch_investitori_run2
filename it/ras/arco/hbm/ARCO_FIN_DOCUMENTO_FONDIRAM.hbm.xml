<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA DOCUMENTI IDENTIFICAZIONE -->
 <class name="it.ras.arco.bean.FinDocumento" table="FIN_DOCUMENTO" where="GSTD_ESIST_B='S'">
  <id name="documentoId" column="DOCUMENTO_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_DOCUMENTO_SEQ</param>
   </generator>
  </id>
  <property name="soggettoId" type="integer" column="SOGGETTO_ID"/>
  <property name="stato" type="string" column="STATO_B" length="20"/>
  <property name="tipoDoc" type="string" column="TIPO_DOC_T" length="3"/>
  <property name="statoVariaz" type="string" column="STATO_VARIAZ_T" length="3"/>
  <property name="daCancellare" type="string" column="DA_CANCELLARE_B" length="1"/>
  <property name="numeroDoc" type="string" column="NUMERO_DOC_S" length="15"/>
  <property name="dataRilascio" type="date" column="DATA_RILASCIO_D"/>
  <property name="autoritaRilasc" type="string"
   column="AUTORITA_RILASC_S" length="35"/>
  <property name="luogoRilascio" type="string" column="LUOGO_RILASCIO_S" length="35"/>
  <property name="provRilascio" type="string" column="PROV_RILASCIO_T" length="3"/>
  <property name="nazioneRilascio" type="string"
   column="NAZIONE_RILASCIO_T" length="3"/>
  <property name="dataScadenza" type="date" column="DATA_SCADENZA_D"/>
  <property name="presNote" type="string" column="PRES_NOTE_B" length="1"/>
  <property name="desDatiSupl" type="string" column="DES_DATI_SUPL" length="20"/>
  <property name="inizioVal" type="date" column="INIZIO_VAL_D"/>
  <property name="fineVal" type="date" column="FINE_VAL_D"/>
  <property name="userUltimaModifica" type="string"
   column="GSTD_ULT_USER_S" length="20"/>
  <property name="proceduraUltimaModifica" type="string"
   column="GSTD_ULT_PROC_S" length="20"/>
  <property name="dataUltimaModifica" type="date" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimento" type="date" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B" length="1"/>
  <property name="utenteInserimento" type="string"
   column="GSTD_USER_INS_S" length="20"/>
  <property name="tipoUltimaModifica" type="string"
   column="GSTD_TIPO_MOD_B" length="1"/>
     <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
 </class>
</hibernate-mapping>
