<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA CRM1 -->
 <class name="it.ras.arco.bean.FinCrm1" table="FIN_CRM1" where="GSTD_ESIST_B='S'">
  <id name="crm1Id" column="CRM1_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_CRM1_SEQ</param>
   </generator>
  </id>
  <property name="promotoreId" type="integer" column="PROMOTORE_ID"/>
  <property name="prodottoId" type="integer" column="PRODOTTO_ID"/>
  <property name="contrattoId" type="integer" column="CONTRATTO_ID"/>
  <property name="movimentoId" type="integer" column="MOVIMENTO_ID"/>
  <property name="codiceProdottoEsterno" type="string" column="COD_PRODOTTO_EXT_S" length="20"/>
  <property name="codiceLineaEsterno" type="string" column="COD_LINEA_EXT_S" length="10"/>
  <property name="numeroContratto" type="string" column="NUM_CONTRATTO_S" length="20"/>
  <property name="numeroMovimentoEsterno" type="string" column="NUM_MOV_EXT_S" length="20"/>
  <property name="modalitaSottoscrizione" type="string" column="MOD_SOTTOSCRIZIONE_B" length="1"/>
  <property name="causaleConferimentoSmobilizzo" type="string" column="CAUSALE_CONF_SMOB_T" length="3"/>
  <property name="causaleProvvigionale" type="string" column="CAUSALE_PROVV_T" length="3"/>
  <property name="dataCompetenza" type="date" column="DATA_COMPETENZA_D"/>
  <property name="dataRegolamento" type="date" column="DATA_REGOLAMENTO_D"/>
  <property name="importoBaseSpese" type="double" column="IMP_BS_C"/>
  <property name="importoPayIn" type="double" column="IMP_PAY_IN_C"/>
  <property name="importoProvvigioniDiretteFront" type="double" column="IMP_DIR_FRONT_C"/>
  <property name="importoProvvigioniOverFront" type="double" column="IMP_OVER_FRONT_C"/>
  <property name="flagDiretteOver" type="string" column="FLG_DIR_OVER_B" length="1"/>
  <property name="societaProdotto" type="string" column="SOCIETA_PRODOTTO_S" length="50"/>
  <property name="tipoProdotto" type="string" column="TIPO_PRODOTTO_T" length="3"/>
  <property name="codiceRete" type="string" column="COD_RETE_T" length="3"/>
  <property name="codiceCompagnia" type="string" column="COD_COMP_T" length="3"/>
  <property name="gradoGerarchico" type="string" column="GRADO_S" length="3"/>
  <property name="userUltimaModifica" type="string" column="GSTD_ULT_USER_S" length="40"/>
  <property name="proceduraUltimaModifica" type="string" column="GSTD_ULT_PROC_S" length="40"/>
  <property name="dataUltimaModifica" type="timestamp" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimentoRecord" type="timestamp" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B" length="1"/>
  <property name="utenteInserimento" type="string" column="GSTD_USER_INS_S" length="40"/>
  <property name="tipoUltimaModifica" type="string" column="GSTD_TIPO_MOD_B" length="1"/>
  <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
  </class>
  
 </hibernate-mapping>

