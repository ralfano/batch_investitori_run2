<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA ANAGRAFICA AGENZIA -->
 
 <class name="it.ras.arco.bean.QuadSecPosizioneDt" table="QUAD_SEC_POSIZIONE_DT" where="GSTD_ESIST_B='S'">
  <id name="flxId" column="FLX_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FLX_QUAD_SEC_POSIZIONE_DT_SEQ</param>
   </generator>
  </id>  
 
<property name="posizioneId"                    column="POSIZIONE_ID"                   type="integer"  />
<property name="contrattoId"                    column="CONTRATTO_ID"                   type="integer"  />
<property name="soggettoId"                     column="SOGGETTO_ID"                    type="integer"  />
<property name="sottodossier"                   column="SOTTODOSSIER"                   type="string" length="30"  />
<property name="quadDataVal"                    column="QUAD_DATA_VAL_D"                type="date"  />
<property name="finPosDataVal"                  column="FIN_POS_DATA_VAL_D"             type="date"  />
<property name="dataOrdine"                 column="DATA_ORDINE"                type="date"  />
<property name="quadCtv"                        column="QUAD_CTV"                       type="double"  />
<property name="finPosQuadCtv"                  column="FIN_POS_QUAD_CTV"               type="double"  />
<property name="quadCambioMedio"                column="QUAD_CAMBIO_MEDIO_C"            type="double"  />
<property name="finPosCambioMedioC"            column="FIN_POS_CAMBIO_MEDIO_C "        type="double"  />
<property name="quadQuantita"                   column="QUAD_QUANTITA_C"                type="double"  />
<property name="finPosQuantita"                 column="FIN_POS_QUANTITA_C"             type="double"  />
<property name="stato"                          column="STATO_B"                        type="string" length="2"  />
<property name="userUltimaModifica"             column="GSTD_ULT_USER_S"                type="string" length="40"  />
<property name="proceduraUltimaModifica"        column="GSTD_ULT_PROC_S"                type="string" length="40"  />
<property name="dataUltimaModifica"             column="GSTD_ULT_MOD_D"                 type="date"  />
<property name="dataInserimento"                column="GSTD_INSE_D"                    type="date"  />
<property name="flagEsistenza"                  column="GSTD_ESIST_B"                   type="string" length="1"  />
<property name="utenteInserimento"              column="GSTD_USER_INS_S"                type="string" length="40"  />
  <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
 </class>
  

</hibernate-mapping>


