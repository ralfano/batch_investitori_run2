<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA DOCUMENTI IDENTIFICAZIONE -->
 <class name="it.ras.arco.bean.FinConvenzione" table="FIN_CONVENZIONE" where="GSTD_ESIST_B='S'">
  <id name="convenzioneId" column="CONVENZIONE_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_CONVENZIONE_SEQ</param>
   </generator>
  </id>
 <property name="attFinId"                     column="ATTIVITA_FIN_ID"                    type="integer"  />
 
<property name="descrizioneConvenzione"         column="DESCRIZIONE_CONVENZIONE_S"      type="string" length="100"  />
<property name="dataInizioValidita"             column="DATA_INIZIO_VALIDITA_D"         type="date"  />
<property name="dataFineValidita"               column="DATA_FINE_VALIDITA_D"           type="date"  />
<property name="userUltimaModifica"             column="GSTD_ULT_USER_S"                type="string" length="40"  />
<property name="proceduraUltimaModifica"        column="GSTD_ULT_PROC_S"                type="string" length="40"  />
<property name="dataUltimaModifica"             column="GSTD_ULT_MOD_D"                 type="timestamp"  />
<property name="dataInserimento"                column="GSTD_INSE_D"                    type="timestamp"  />
<property name="flagEsistenza"                  column="GSTD_ESIST_B"                   type="string" length="1"  />
<property name="utenteInserimento"              column="GSTD_USER_INS_S"                type="string" length="40"  />
<property name="tipoUltimaModifica"             column="GSTD_TIPO_MOD_B"                type="string" length="1"  />
<property name="prodottoId"                     column="PRODOTTO_ID"                    type="integer"  />
<property name="tipoProdotto"                   column="TIPO_PRODOTTO"                  type="string" length="4"  />
<property name="codiceConvenzione"              column="CODICE_CONVENZIONE"             type="string" length="10"  />
<property name="descrizioneProdotto"            column="DESCRIZIONE_PRODOTTO_S"         type="string" length="100"  />
<property name="codiceFascia"                   column="CODICE_FASCIA"                  type="string" length="2"  />
<property name="descrizioneFascia"              column="DESCRIZIONE_FASCIA"             type="string" length="125"  />
<property name="flagTipoConvenzione"            column="FLAG_TIPO_CONVENZIONE_C"        type="string" length="1"  />
<property name="dataInizioCommerciale"          column="DATA_INIZIO_COMMERCIALE_D"      type="date"  />
<property name="dataFineCommerciale"            column="DATA_FINE_COMMERCIALE_D"        type="date"  />
 <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  

 </class>
 
 <query name="GetConvenzioneByAttFinIdCodiceConvenzione">from it.ras.arco.bean.FinConvenzione a where a.attFinId=? and a.codiceConvenzione=?
 </query>
</hibernate-mapping>

