<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA CONSENSO PRIVACY -->
 <class name="it.ras.arco.bean.FinSoggettoPrivacy"
	table="FIN_SOGG_PRIVACY" where="GSTD_ESIST_B='S'"
	batch-size="1"
  >
  
  <id name="soggPrivacyId" column="SOGG_PRIVACY_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_SOGG_PRIVACY_SEQ</param>
   </generator>
  </id>

  <property name="tipoPrivacy" type="string" column="TIPO_PRIVACY_T" length="3"/>
  <property name="dataCons" type="date" column="DATA_CONS_D"/>
  <property name="consPrivacy0" type="string" column="CONS_PRIVACY0_B" length="1"/>
  <property name="consPrivacy1" type="string" column="CONS_PRIVACY1_B" length="1"/>
  <property name="consPrivacy2" type="string" column="CONS_PRIVACY2_B" length="1"/>
  <property name="consPrivacy3" type="string" column="CONS_PRIVACY3_B" length="1"/>
  <property name="consPrivacy4" type="string" column="CONS_PRIVACY4_B" length="1"/>
  <property name="consPrivacy5" type="string" column="CONS_PRIVACY5_B" length="1"/>
  <property name="consPrivacy6" type="string" column="CONS_PRIVACY6_B" length="1"/>
  <property name="consPrivacy7" type="string" column="CONS_PRIVACY7_B" length="1"/>
  <property name="consPrivacy8" type="string" column="CONS_PRIVACY8_B" length="1"/>
  <property name="consPrivacy9" type="string" column="CONS_PRIVACY9_B" length="1"/>
  <property name="derogaSegrBanc" type="string"
   column="DEROGA_SEGR_BANC_B" length="1"/>
  <property name="dataDerogaSegrBanc" type="date" column="DATA_DEROGA_SEGR_BANC_D"/>

  <property name="dataConflittoInteressi" type="timestamp" column="DATA_CONF_INT_D"/>
  <property name="regConflittoInteressi" type="string" column="REG_CONF_INT_B" length="1"/>
  <property name="dataAutorizMarketing" type="timestamp" column="DATA_AUT_MRK_D"/>
  <property name="concAutorizMarketing" type="string" column="CONC_AUT_B" length="1"/>
  <property name="dataCompDocRischi" type="timestamp" column="DATA_COMP_DOC_RISK_D"/>
  <property name="compDocRischi" type="string" column="COMP_DOC_RISK_B" length="1"/>
  <property name="attEconomPrisma" type="string" column="ATT_ECO_PRISMA_B" length="1"/>
  

  <property name="userUltimaModifica" type="string"
   column="GSTD_ULT_USER_S" length="20"/>
  <property name="proceduraUltimaModifica" type="string"
   column="GSTD_ULT_PROC_S" length="20"/>
  <property name="dataUltimaModifica" type="timestamp" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimento" type="timestamp" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B" length="1"/>
  <property name="utenteInserimento" type="string"
   column="GSTD_USER_INS_S" length="20"/>
  <property name="tipoUltimaModifica" type="string"
   column="GSTD_TIPO_MOD_B" length="1"/>
   <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
   
   <many-to-one name="soggetto" lazy="proxy" class="it.ras.arco.bean.FinSoggetto"  
		insert="true" update="true"  column="SOGGETTO_ID" unique="true" not-null="true"
   		fetch="select"   			
		not-found="ignore" cascade="lock"/>
   
 </class>
</hibernate-mapping>

