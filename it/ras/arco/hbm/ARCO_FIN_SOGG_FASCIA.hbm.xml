<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA CONSENSO PRIVACY -->
 <class name="it.ras.arco.bean.FinSoggFascia"
  table="FIN_SOGG_FASCIA" where="GSTD_ESIST_B='S'">
  <id name="soggFasciaId" column="SOGG_FASCIA_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_SOGG_FASCIA_SEQ</param>
   </generator>
  </id>
<property name="soggettoId"                        column="SOGGETTO_ID"                       type="integer"  />
  
<property name="codSogg"                        column="COD_SOGG"                       type="integer"  />
<property name="navPrimLiv"                     column="NAV_PRIM_LIV"                   type="double"  />
<property name="navSecoLiv"                     column="NAV_SECO_LIV"                   type="double"  />
<property name="fascCalc"                       column="FASC_CALC"                      type="string" length="1"  />
<property name="fascEred"                       column="FASC_ERED"                      type="string" length="1"  />
<property name="fascSec"                        column="FASC_SEC"                       type="string" length="1"  />
<property name="dataSec"                        column="DATA_SEC"                       type="date"  />
<property name="fascAsse"                       column="FASC_ASSE"                      type="string" length="1"  />
<property name="causAsse"                       column="CAUS_ASSE"                      type="string" length="2"  />
<property name="dataAsse"                       column="DATA_ASSE"                      type="date"  />
<property name="numContCorr"                    column="NUM_CONT_CORR"                  type="integer"  />
<property name="numIncoe"                       column="NUM_INCOE"                      type="integer"  />
<property name="dataIncoe"                      column="DATA_INCOE"                     type="date"  />
<property name="flgUpDown"                      column="FLG_UP_DOWN"                    type="string" length="1"  />
<property name="dataUpDown"                     column="DATA_UP_DOWN"                   type="date"  />
<property name="flgConsPriv"                    column="FLG_CONS_PRIV"                  type="string" length="1"  />
<property name="codCliePorfCcor"                column="COD_CLIE_PORF_CCOR"             type="integer"  />
<property name="datInse"                        column="DAT_INSE"                       type="date"  />
<property name="datVarz"                        column="DAT_VARZ"                       type="date"  />
<property name="codUltiUser"                    column="COD_ULTI_USER"                  type="string" length="8"  />
<property name="tmpUltiVarz"                    column="TMP_ULTI_VARZ"                  type="date"  />
<property name="flgVali"                        column="FLG_VALI"                       type="string" length="1"  />
<property name="userUltimaModifica"             column="GSTD_ULT_USER_S"                type="string" length="40"  />
<property name="proceduraUltimaModifica"        column="GSTD_ULT_PROC_S"                type="string" length="40"  />
<property name="dataUltimaModifica"             column="GSTD_ULT_MOD_D"                 type="timestamp"  />
<property name="dataInserimento"                column="GSTD_INSE_D"                    type="timestamp"  />
<property name="flagEsistenza"                  column="GSTD_ESIST_B"                   type="string" length="1"  />
<property name="utenteInserimento"              column="GSTD_USER_INS_S"                type="string" length="40"  />
<property name="tipoUltimaModifica"             column="GSTD_TIPO_MOD_B"                type="string" length="1"  />
<property name="dataInizio"                     column="DATA_INIZIO"                    type="date"  />
<property name="dataFine"                       column="DATA_FINE"                      type="date"  />
<property name="flagAttivo"                     column="FLAG_ATTIVO"                    type="string" length="1"  />

  <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  

   <!--many-to-one name="soggetto" lazy="false" class="it.ras.arco.bean.FinSoggetto"  
   insert="true" update="true"  column="SOGGETTO_ID" unique="true" not-null="true"/-->
   
 </class>
</hibernate-mapping>

