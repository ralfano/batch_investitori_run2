<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

 <class name="it.ras.arco.bean.FinLegameIndir" batch-size="1" table="FIN_LEGAME_INDIR" where="GSTD_ESIST_B='S'">
 
  <id name="legameIndirId" column="LEGAME_INDIR_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_LEGAME_INDIR_SEQ</param>
   </generator>
  </id>
 
 
  <property name="tipoGestione" type="string" column="TIPO_GESTIONE_T" length="4"/>
  <property name="tipoIndirizzo" type="string" column="TIPO_INDIRIZZO_T" length="4"/>
  <property name="provDatiAnag" type="string" column="PROV_DATI_ANAG_T" length="10"/>
  <property name="descrizione" type="string" column="DESCRIZIONE_S" length="100"/>
  <property name="dataElaborazione" type="timestamp" column="DATA_ELAB_D"/>
  <property name="userUltimaModifica" type="string" column="GSTD_ULT_USER_S" length="20"/>
  <property name="proceduraUltimaModifica" type="string" column="GSTD_ULT_PROC_S" length="20"/>
  <property name="dataUltimaModifica" type="timestamp" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimento" type="timestamp" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B" length="1"/>
  <property name="utenteInserimento" type="string" column="GSTD_USER_INS_S" length="20"/>
  <property name="tipoUltimaModifica" type="string" column="GSTD_TIPO_MOD_B" length="1"/>
  <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="20" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="20" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
 <!-- RELAZIONE CON LA FIN_CONTRATTO -->
  <many-to-one name="contratto" 
  			class="it.ras.arco.bean.FinContratto" column="CONTRATTO_ID"  
  			cascade="lock" fetch="select" not-found="ignore" lazy="proxy" />
 
 <!-- RELAZIONE CON LA FIN_SOGGETTO -->
  <many-to-one name="soggetto" 
  			class="it.ras.arco.bean.FinSoggetto" column="SOGGETTO_ID"  
  			cascade="lock" fetch="select" not-found="ignore" lazy="proxy" />
 
 <!-- RELAZIONE CON LA FIN_INDIRIZZO -->
  <many-to-one name="indirizzo" 
  			class="it.ras.arco.bean.FinIndirizzo" column="INDIRIZZO_ID"  
  			cascade="lock" fetch="select" not-found="ignore" lazy="proxy" />
 
 <!-- RELAZIONE CON LA FIN_INDIRIZZO (correlato) -->
  <many-to-one name="indirizzoCorrelato" 
  			class="it.ras.arco.bean.FinIndirizzo" column="INDIRIZZO_COR_ID"  
  			cascade="lock" fetch="select" not-found="ignore" lazy="proxy" />

 </class>
 


</hibernate-mapping>
