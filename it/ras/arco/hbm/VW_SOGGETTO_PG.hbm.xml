	<sql-query name="OutboundVistaSoggetto">
	 <return alias="soggetto" class="it.ras.arco.bean.VwSoggettoPg"/>
		select 
		distinct {soggetto.*} from
		FIN_SOGGETTO_PG soggetto where soggetto.FL_INVIATO = 'N' 
	</sql-query>
	
	
