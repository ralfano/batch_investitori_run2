<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA ANAGRAFICA AGENZIA -->
 <class name="it.ras.arco.bean.FinAgenzia" table="FIN_AGENZIA" where="GSTD_ESIST_B='S'">
  <id name="agenziaId" column="AGENZIA_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_AGENZIA_SEQ</param>
   </generator>
  </id>  
  <property name="citta" type="string" column="CITTA_S"/>
  <property name="codAgenzia" type="string" column="COD_AGENZIA_S"/>
  <property name="tipologia" type="string" column="TIPOLOGIA_T"/>
  <property name="dataInizioValidita" type="date" column="DATA_INIZIO_D"/>
  <property name="dataFineValidita" type="date" column="DATA_FINE_D"/>  
  <property name="indirizzo" type="string" column="INDIRIZZO_S"/>  
  <property name="cap" type="string" column="CAP_S"/>  
  <property name="prefisso" type="string" column="TEL_PREFISSO_S"/>  
  <property name="telefono1" type="string" column="TEL_NUMERO_1_S"/>  
  <property name="telefono2" type="string" column="TEL_NUMERO_2_S"/>  
  <property name="telefono3" type="string" column="TEL_NUMERO_3_S"/> 
  <property name="fax" type="string" column="FAX_S"/>  
  <property name="codRete" type="string" column="COD_RETE_T"/>  
  <property name="codCompagnia" type="string" column="COD_COMPAGNIA_T"/>  
  <property name="regioneCommerciale" type="string" column="REGIONE_COMM_T"/>  
  <property name="provincia" type="string" column="PROV_S"/>  
  <property name="desAgenzia" type="string" column="DES_AGENZIA_S"/>  
  <property name="telefono1Ext" type="string" column="TELEFONO_1_EXT_S"/>  
  <property name="telefono2Ext" type="string" column="TELEFONO_2_EXT_S"/>  
  <property name="telefonoExt3" type="string" column="TELEFONO_3_EXT_S"/>  
  <property name="faxExt" type="string" column="FAX_EXT"/>  
  <property name="userUltimaModifica" type="string" column="GSTD_ULT_USER_S" length="20"/>
  <property name="proceduraUltimaModifica" type="string" column="GSTD_ULT_PROC_S" length="20"/>
  <property name="dataUltimaModifica" type="timestamp" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimento" type="timestamp" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B" length="1"/>
  <property name="utenteInserimento" type="string" column="GSTD_USER_INS_S" length="20"/>
  <property name="tipoUltimaModifica" type="string" column="GSTD_TIPO_MOD_B" length="1"/>
  <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
 </class>
  <!-- QUERY -->
 <query name="GetAgenziaByCodice">from it.ras.arco.bean.FinAgenzia a where a.codAgenzia = ?</query>
 
</hibernate-mapping>


