<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA EMITTENTE -->
 <class name="it.ras.arco.bean.DecRegoleInboundMovimento" table="DEC_REGOLE_INBOUND_MOVIMENTO" where="GSTD_ESIST_B='S'">
  <id name="decRegoleInboundId" column="DEC_REGOLE_INBOUND_ID"  type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_EMITTENTE_SEQ</param>
   </generator>
  </id>
<property name="codiceEmittente"                column="CODICE_EMITTENTE"               type="string" length="5"  	/>
<property name="causaleMovimentoDecod"          column="CAUSALE_MOVIMENTO_DECOD"        type="string" length="40"  	/>
<property name="beanGetContratto"               column="BEAN_GET_CONTRATTO"             type="string" length="40"  	/>
<property name="prodAttFinDecod"                column="PROD_ATT_FIN_DECOD"             type="string" length="40"  	/>
<property name="emittenteDecod"                 column="EMITTENTE_DECOD"                type="string" length="40"   />
<property name="verificaTipoQuote" 				column="VERIFICA_TIPO_QUOTE" 			type="integer"				/>
<property name="beanGetAttFinProdotto"          column="BEAN_GET_ATT_FIN_PRODOTTO"      type="string" length="40"  	/>
<property name="controllaOrdine" 				column="CONTROLLA_ORDINE" 				type="integer"				/>
<property name="verificaEmittente" 				column="VERIFICA_EMITTENTE" 			type="integer"				/>
<property name="convertiValuta" 				column="CONVERTI_VALUTA" 			    type="integer"				/>
<property name="calcolaImportoNetto"			column="CALCOLA_IMPORTO_NETTO" 			type="integer"				/>
<property name="movimentiIniziativa"			column="MOVIMENTI_INIZIATIVA" 			type="integer"				/>
<property name="regolaAttiva"					column="REGOLA_ATTIVA" 					type="integer"				/>
<property name="descrizioneRegola"              column="DESCRIZIONE_REGOLA"   			type="string" length="40"  	/>
<property name="userUltimaModifica"  			column="GSTD_ULT_USER_S" 				type="string" length="20"	/>
<property name="proceduraUltimaModifica" 		column="GSTD_ULT_PROC_S" 				type="string" length="20"	/>
<property name="dataUltimaModifica" 			column="GSTD_ULT_MOD_D" 				type="date" 				/>
<property name="dataInserimento" 				column="GSTD_INSE_D" 					type="date" 				/>
<property name="flagEsistenza" 					column="GSTD_ESIST_B" 					type="string" length="1"	/>
<property name="utenteInserimento" 				column="GSTD_USER_INS_S" 				type="string" length="20"	/>
<property name="tipoUltimaModifica" 			column="GSTD_TIPO_MOD_B" 				type="string" length="1"	/> 
<property name="estraiContrattoDaOrdine"		column="ESTRAI_CONTRATTO_DA_ORDINE"		type="integer"				/>
<property name="controllaMovimentoPrecedente"	column="CONTROLLA_MOVIMENTO_PRECEDENTE"	type="integer"				/>
<property name="rollbackMovimentoPrecedente"	column="ROLLBACK_MOVIMENTO_PRECEDENTE"	type="integer"				/>
<property name="creaOrdine"						column="CREA_ORDINE"					type="integer"				/>
<property name="calcolaMovimento"				column="CALCOLA_MOVIMENTO"				type="string" length="50"  	/>
<property name="decMezzoPag"				    column="DEC_MEZZO_PAG"				    type="string" length="20"  	/>




</class>

<!-- QUERY -->
<query name="GetRegoleByCodiceEmittente">select s from it.ras.arco.bean.DecRegoleInboundMovimento s where s.codiceEmittente = :codEmitt and s.regolaAttiva=1</query>
</hibernate-mapping>

