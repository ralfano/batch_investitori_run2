<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- TABELLA STRUTTURA PROMOTORI -->
 <class name="it.ras.arco.bean.FinPromotoreStruttura" table="FIN_PROMOTORE_STRUTTURA" where="GSTD_ESIST_B='S'">
  <id name="promotoreStrutturaId" column="PROMOTORE_STRUTTURA_ID" type="integer">
   <generator class="sequence">
    <param name="sequence">FIN_PROMOTORE_STRUTTURA_SEQ</param>
   </generator>
  </id>
  
  <property name="promotoreFiglioId" type="integer" column="PROMOTORE_FIGLIO_ID"/>
  <property name="promotorePadreId" type="integer" column="PROMOTORE_PADRE_ID"/>
  <property name="codiceIstituto" type="string" column="COD_ISTITUTO_S" length="5"/>
  <property name="livelloStrutturaFiglio" type="string" column="LIVELLO_STRUT_FIGLIO" length="3"/>
  <property name="livelloStrutturaPadre" type="string" column="LIVELLO_STRUT_PADRE" length="3"/>
  <property name="regioneCommerciale" type="string" column="REGIONE_COMM_T" length="3"/>
  <property name="distretto" type="string" column="DISTRETTO_S" length="10"/>
  <property name="dataInizio" type="date" column="INIZIO_D"/>
  <property name="dataFine" type="date" column="FINE_D"/>
  <property name="userUltimaModifica" type="string" column="GSTD_ULT_USER_S" length="40"/>
  <property name="proceduraUltimaModifica" type="string" column="GSTD_ULT_PROC_S" length="40"/>
  <property name="dataUltimaModifica" type="date" column="GSTD_ULT_MOD_D"/>
  <property name="dataInserimentoRecord" type="date" column="GSTD_INSE_D"/>
  <property name="flagEsistenza" type="string" column="GSTD_ESIST_B" length="1"/>
  <property name="utenteInserimento" type="string" column="GSTD_USER_INS_S" length="40"/>
  <property name="tipoUltimaModifica" type="string" column="GSTD_TIPO_MOD_B" length="1"/>
   <property name="userUltimaModificaSlave" type="string" column="GSTD_ULT_USER_S" length="40" insert="false" update="false"/>
  <property name="proceduraUltimaModificaSlave" type="string" column="GSTD_ULT_PROC_S" length="40" insert="false" update="false"/>
  <property name="dataUltimaModificaSlave" type="timestamp" column="GSTD_ULT_MOD_D" insert="false" update="false"/>
  <property name="tipoUltimaModificaSlave" type="string" column="GSTD_TIPO_MOD_B" length="1" insert="false" update="false"/>
  
    
 </class>
 
 <query name="GetPromotoreStruttura">from it.ras.arco.bean.FinPromotoreStruttura p where p.promotorePadreId = ? and p.promotoreFiglioId = ? and p.dataInizio = ? </query>
 
 </hibernate-mapping>
  
