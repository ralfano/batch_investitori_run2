<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="it.ras.arco.bean.SnpEstrazioni" table="SNP_ESTRAZIONI" where="FL_INVIATO='S'">

	<id name="idEstrazioni" column="ID_SNP_ESTRAZIONI" type="integer">
		<generator class="sequence">
			<param name="sequence">SNP_ESTRAZIONI_SEQ</param>
		</generator>
	</id>


	<property name="dataInserito"         column="DATA_INSERITO"          type="date" />
	<property name="dataInviato"              column="DATA_INVIATO"               type="date" />
	<property name="id"         column="ID"          type="integer"   />
	<property name="tableName"      column="TABLE_NAME"      type="string" />
	<property name="dest"      column="DEST"      type="string" />
	<property name="inviato"      column="FL_INVIATO"      type="string" />
	<property name="dataTxt"      column="DATA_TXT"      type="string" />




</class>







</hibernate-mapping>

